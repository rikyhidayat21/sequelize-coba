<%- include('Partial/header') %>
<body>
    <h1 style="text-align: center;"><b>Students Data</b></h1>
    <h3 style="text-align: center;"> <a href="/students/add">add student</a> </h3>
    <a href="/">Back to school information</a>
    <table style="text-align: left;" border="1">
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Options</th>
        </tr>
        <% data.forEach(el => { %>
            <tr>
                <td><%=el.id%></td>
                <td><%=el.first_name%></td>
                <td><%=el.last_name%></td>
                <td><%=el.email%></td>
                <td> <a href="/students/<%=el.id%>/edit">Edit</a> | <a href="/students/<%=el.id%>/delete">Delete</a> </td>
            </tr>
        <% })%>
    </table>
</body>
</html> 