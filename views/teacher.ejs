<%- include('Partial/header') %>
<body>
    <h1 style="text-align: center;"><b>Teachers Data</b></h1>
    <a href="/">back to school information</a><br>
    <table style="text-align: left;" border="1">
        <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        <% data.forEach(el => { %>
            <tr>
                <td><%=el.id%></td>
                <td><%=el.first_name%></td>
                <td><%=el.last_name%></td>
                <td><%=el.email%></td>
            </tr>
        <% })%>
    </table>
</body>
</html> 